summaries:
  Unique: values.unique().length
formulas:
  image: image(file.file)
  first-image: image(file.embeds[0])
  Second_Image: image(file.embeds[1])
  third_Image: image(file.embeds[2])
  nick group: date.month
  yearMonth: date(note["journal-date"]).format("YYYYMM")
  on_this_day: |
    if(date(this.file.name).month ==
    date(file.name).month,
    if(date(this.file.name).day ==
    date(file.name).day,
    if(date(this.file.name).year >
    date(file.name).year, "True",
    "False"),
    "False"),
    "False")
  test: date(this.file.name).day
properties:
  formula.image:
    displayName: Image
  formula.Second_Image:
    displayName: second_Image
  note.log-healthy-eating:
    displayName: ü•ó log-healthy-eating
  note.log-day-rating:
    displayName: ‚ùé log-day-rating
  note.log-sleep-hours:
    displayName: üò¥ log-sleep-hours
  note.log-sleep-rating:
    displayName: üõèÔ∏è log-sleep-rating
views:
  - type: table
    name: All Journal Notes
    filters:
      and:
        - type.contains("daily")
    groupBy:
      property: formula.yearMonth
      direction: ASC
    order:
      - file.name
      - summary
      - formula.first-image
      - formula.Second_Image
      - formula.third_Image
      - voicenote
      - file.tags
      - formula.yearMonth
      - file.path
    sort: []
    summaries:
      log-healthy-eating: Average
      log-day-rating: Average
      log-sleep-hours: Average
      log-sleep-rating: Average
    columnSize:
      file.name: 162
      note.summary: 281
      formula.first-image: 149
      formula.Second_Image: 149
      formula.third_Image: 150
      note.voicenote: 230
    rowHeight: extra
  - type: table
    name: Birthday This Month
    filters:
      and:
        - date(birthday).month == date(this.file.name).month
    order:
      - file.name
      - birthday
      - summary
    sort: []
    summaries:
      log-healthy-eating: Average
      log-day-rating: Average
      log-sleep-hours: Average
      log-sleep-rating: Average
    columnSize:
      file.name: 162
      note.summary: 281
      formula.first-image: 149
      formula.Second_Image: 149
      formula.third_Image: 150
      note.voicenote: 230
    rowHeight: medium
  - type: table
    name: Today Years Ago
    filters:
      and:
        - formula.on_this_day.contains("True")
    order:
      - file.name
      - summary
      - formula.first-image
      - formula.Second_Image
      - formula.third_Image
      - voicenote
    sort:
      - property: file.name
        direction: ASC
    summaries:
      log-healthy-eating: Average
      log-day-rating: Average
      log-sleep-hours: Average
      log-sleep-rating: Average
    columnSize:
      file.name: 162
      note.summary: 281
      formula.first-image: 131
      formula.Second_Image: 125
      formula.third_Image: 130
      note.voicenote: 161
      formula.on_this_day: 112
      formula.test: 125
    rowHeight: extra
  - type: table
    name: Notes Created This Day
    filters:
      and:
        - file.ctime == date(this.file.name)
        - '!file.folder.startsWith("Journal")'
        - '!file.folder.contains("Attachment")'
    order:
      - file.name
      - summary
      - file.ctime
      - file.mtime
      - file.folder
    sort: []
    summaries:
      log-healthy-eating: Average
      log-day-rating: Average
      log-sleep-hours: Average
      log-sleep-rating: Average
    columnSize:
      file.name: 162
      note.summary: 281
      formula.first-image: 149
      formula.Second_Image: 149
      formula.third_Image: 150
      note.voicenote: 230
    rowHeight: medium
  - type: table
    name: Weekly Note
    filters:
      and:
        - ((date(note["journal-date"]) - date(this.note["journal-date"])) / (1000 * 60 * 60 * 24)) < 7
        - ((date(note["journal-date"]) - date(this.note["journal-date"])) / (1000 * 60 * 60 * 24)) >= 0
        - or:
            - type.contains("daily")
    order:
      - file.name
      - log-day-rating
      - summary
      - formula.first-image
      - formula.Second_Image
      - formula.third_Image
      - voicenote
      - file.tags
    sort:
